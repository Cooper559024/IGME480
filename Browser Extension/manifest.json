{
  "name": "Bad T",
  "short_name": "Bad T",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Replaces the letter T with a red Comic Neue T.",
  "author": "Aidan McMurray",

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
